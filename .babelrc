{
  "presets": ["es2015", "react", "stage-0"],
  "plugins": [
    "react-hot-loader/babel",
    [
      "transform-runtime",
      "transform-class-properties",
      {
        "helpers": false,
        "polyfill": false,
        "regenerator": true,
        "moduleName": "babel-runtime"
      }
    ]
  ],
  "env": {
    "production":{
      "preset":["react-optimize"]
    }
  }
}
